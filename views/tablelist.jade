extends layout

block content
    .title
        h1#myTable= tableName
    
    form#list-form
      div#main
        table#list.table
          thead
            tr
              for hdr in Object.keys(tablelist[0])
                th #{hdr}
          tbody
            if tablelist && tablelist.length > 0
              each row in tablelist
                tr
                  each col in row
                    td #{col}
